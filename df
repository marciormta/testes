import pandas as pd
import matplotlib.pyplot as plt
import seaborn as sns

# Importar os dados do Excel
df = pd.read_excel('caminho/para/o/arquivo.xlsx')

# Gerar o gráfico de dispersão
sns.scatterplot(data=df, x='qtd_acionamento', y='status_do_acordo', hue='tipo_acionamento')

# Adicionar títulos e legendas
plt.title('Relação entre Quantidade de Acionamentos e Status do Acordo')
plt.xlabel('Quantidade de Acionamentos')
plt.ylabel('Status do Acordo')
plt.legend(title='Canal Associado')
plt.show()
